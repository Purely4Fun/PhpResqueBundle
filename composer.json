{
    "require": {
        "chrisboulton/php-resque": "master-dev"
    }
    
    "repositories": {
        "chrisboulton/php-resque": {
            "package": {
                "name": "chrisboulton/php-resque",
                "version": "master-dev",
                "require": {
                    "php": ">=5.3.2"
                },
                "dist": {
                    "url":       "https://github.com/chrisboulton/php-resque/zipball/master",
                    "type":      "zip"
                },
                "source": {
                    "url":       "git://github.com/chrisboulton/php-resque.git",
                    "type":      "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Resque": "lib/" }
                }
            }
        }
    }
}